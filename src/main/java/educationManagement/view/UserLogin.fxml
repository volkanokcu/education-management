<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane focusTraversable="true" prefHeight="217.0" prefWidth="252.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="educationManagement.controller.UserLoginController">
   <children>
      <Label fx:id="statusLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="14.0" prefHeight="40.0" prefWidth="200.0" text="Lütfen Formu Doldurun" textFill="#b21717">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="joinBtn" layoutX="30.0" layoutY="153.0" mnemonicParsing="false" onAction="#buttonEvent" prefHeight="40.0" prefWidth="90.0" text="Giriş">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitBtn" layoutX="140.0" layoutY="153.0" mnemonicParsing="false" onAction="#buttonEvent" prefHeight="40.0" prefWidth="90.0" text="Çıkış">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="userNo" alignment="CENTER" layoutX="30.0" layoutY="54.0" prefHeight="40.0" prefWidth="200.0" promptText="Kullanıcı No">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" alignment="CENTER" layoutX="30.0" layoutY="105.0" prefHeight="40.0" prefWidth="200.0" promptText="Şifre">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
     
   </children>
</AnchorPane>
